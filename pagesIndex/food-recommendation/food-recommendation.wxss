/* pagesIndex/food-recommendation/food-recommendation.wxss */
.page{
    background-color: #f2f2f2;
    box-sizing: border-box;
    padding: 0 40rpx;
    position: absolute;
    width: 100%;
    min-height: 100%;
}
.recommendation-date{
    box-sizing: border-box;
    padding: 20rpx;
    background: #fff;
    position: relative;
    top: -150rpx;
    border-radius:10px;
}
.recommendation-top{
    background: #ED6F69;
    width: 100%;
    height: 200rpx;
    box-sizing: border-box;
    position: relative;
}
.recommendation-swiperItem{
    display: flex;
    align-content: center;
    align-items: center;
    justify-content: space-between;
}
.swiperItem{
    line-height: 60rpx;
    text-align: center;
    width: 72rpx;
    height: 120rpx;
}
.swiperItemR{
    background:linear-gradient(0deg,rgba(246,120,114,1),rgba(248,176,170,1));
    box-shadow:0rpx 8rpx 20rpx 0rpx rgba(237,111,105,0.2);
    border-radius:20rpx;
    color:rgba(255,255,255,1);
}
.swiperItemW{
    color:rgba(69,69,69,1);
}
.swiperItemNR{
    font-size:34rpx;
    font-family:DIN Condensed;
    font-weight:bold;
    color:rgba(255,255,255,1);
}
.swiperItemNW{
    font-size:34rpx;
    font-family:DIN Condensed;
    font-weight:bold;
    color:rgba(69,69,69,1);
}
.swiperItemTR{
    font-size:20rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1);
}
.swiperItemTW{
    font-size:20rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(155,155,155,1);
}
.recommendation-month{
    font-weight:bold;
    color:rgba(69,69,69,1);
    text-align: center;
    margin: 20rpx 0rpx;
}
.recommendation-content{
    width: 100%;
    box-sizing: border-box;
    padding: 50rpx 30rpx 30rpx;
    background: #fff;
    border-radius:10px;
    margin-bottom: 20rpx;
    position: relative;
}
.recommendation-tj{
    width: 100%;
    height: 166rpx;
    background-image:url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    margin-bottom: 20rpx;
    padding-top: 47rpx;
    box-sizing: border-box;
}
.recommendation-tj-text1{
    font-size:30rpx;
    font-family:PingFang SC;
    font-weight:600;
    color:rgba(51,51,51,1);
    margin-left: 134rpx;
    margin-bottom: 20rpx;
}
.recommendation-tj-text2{
    font-size:22rpx;
    font-family:PingFang SC;
    font-weight:400;
    margin-left: 134rpx;
    color:rgba(69,69,69,1);
}
.content-title{
    font-size:36rpx;
    font-family:PingFang SC;
    font-weight:bold;
    color:rgba(51,51,51,1);
    margin-left: 25rpx;
}
.content-kcal{
    width: 185rpx;
    text-align: center;
    font-size:24rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(69,69,69,1);
    z-index: 90;
}
.food-title{
    font-size:30rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(69,69,69,1);
    margin-bottom: 10rpx;
}
.food-kcal{
    font-size:24rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(160,160,160,1);
}
.food-quantityDesc{
    font-size:24rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(160,160,160,1);
}
.recommendation-content-bottom{
    margin-bottom: 40rpx;
}
.recommendation-line{
    width: 145rpx;
    height: 12rpx;
    background: rgba(254,236,235,1);
    position: absolute;
    right: 23rpx;
    top: 75rpx;
    z-index: 0;

}
.swiperChange{
    text-align: center;
    padding-bottom: 20rpx;
    height: 50rpx;
}

.swiperText{
    font-size:22rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(237,111,105,1);
}
.recommendation-text{
    text-align: center;
    font-size:36prx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(69,69,69,1);
    margin: 40rpx 0rpx;
}
