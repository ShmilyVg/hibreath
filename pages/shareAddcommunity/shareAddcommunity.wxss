    margin-bottom: 105rpx;
}
.shareAdd-Img{
    width:263rpx;
    height:263rpx;
    border-radius:50%;
    margin-bottom: 110rpx;
}
.shareAdd-Btn{
    width:500rpx;
    height:100rpx;
