/* pages/device-bind/device-bind.wxss */
page{

}
.page{
    min-height: 100%;
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
    padding: 0 50rpx;
    box-sizing:border-box;
}

.home-orangebtn{
    width: 500rpx;
    height: 100rpx;
    color: #EE6F69;
    font-size: 30rpx;
    text-align: center;
    line-height: 100rpx;
    background: #fff;
    border-radius: 50rpx;
    margin: 80rpx auto 0rpx auto;
}
.bind-hint{
    color: #8C8C8C;
    fongt-size:28rpx;
    text-align: center;
    margin-top: 10rpx;
}
.noBindText{
    font-family: PingFang-SC;
    font-size: 50rpx;
    color: #fff;
}
.home-title{
    font-size: 50rpx;
    font-weight: 700;
    color: #AAAAAA;
    text-align: center;
}

.home-p{
    font-family: PingFang-SC;
    color: #FFFFFF;
    font-size: 30rpx;
}
.findImd{
    width: 320rpx;
    height: 510rpx ;
    background-image:url("data:image/png;base64,");
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin: 140rpx auto 70rpx auto;
}
.findedImd{
    width: 750rpx;
    height: 774rpx ;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin: 140rpx auto 70rpx auto;
    animation:myFirst 2s linear infinite;
}
@keyframes myFirst
{
    0%    {background-image: url("http://backend.hipee.cn/assets/home/images/hb/findWhite.png") }
    20%   {background-image: url("http://backend.hipee.cn/assets/home/images/hb/findWhite.png") }
    100%  {background-image: url("http://backend.hipee.cn/assets/home/images/hb/findRed.png") }
}
.contentTextB{
    font-family: PingFang-SC;
    font-size: 50rpx;
    color: #454545;
    text-align: center;
    margin-bottom: 20rpx;
}
.contentTextS{
    font-family: PingFang-SC;
    font-size: 32rpx;
    color: #A0A0A0;
    text-align: center;
}
.nofind-img{
    width: 36rpx;
    height: 36rpx;
}
.bindSuccess-top{
    width: 100%;
    height: 431rpx;
    text-align: center;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");

}
.bind-text{
    font-family: PingFang-SC;
    color: #fff;
    font-size: 50rpx;
}
.bindSuccess-img{
    width: 200rpx;
    height: 200rpx;
  /*  margin-top: 40rpx;
    margin-bottom: 25rpx;*/
    margin: 40rpx auto 25rpx auto;
}
.bindSuccess-bto{
    padding: 80rpx 70rpx 0rpx 70rpx;
    position: relative;
    top: -60rpx;
    border-radius: 60rpx 60rpx 0 0;
    border: 1px #fff solid;
    z-index: 2;
    background: #fff;
}
.bindSuccess-btoText1{
    font-family: PingFang-SC;
    font-size: 40rpx;
    color: #454545;
    text-align: center;
    margin-bottom: 30rpx;
}
.bindSuccess-btoText2{
    font-family: PingFang-SC;
    font-size: 30rpx;
    color: #7C7C7C;
    text-align: center;

}
.bindSuccess-btoLine{
    margin-top: 55rpx;
    width: 100%;
    border: 0.5rpx #f2f2f2 solid;

}
.slide-text{
    font-family: PingFang-SC;
    font-size: 30rpx;
    color: #7C7C7C;

}
.bindSuccess-swiper{
    text-align: center;
    padding: 92rpx 40rpx 0rpx 40rpx;
}
/*用来包裹所有的小圆点 */
.dots {
    width: 100%;
    height: 5rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-top: -80rpx;
}
/*未选中时的小圆点样式 */
.dot {
    width: 15rpx;
    height: 15rpx;
    border-radius: 50%;
    background-color:#D8D8D8;
    margin-right: 30rpx;
}
/*选中以后的小圆点样式 */
.active {
    width: 15rpx;
    height: 15rpx;
    margin-right: 30rpx;
    border-radius: 50%;
    background-color:#EE6F69;
}
.bindSuccess-btn{
    width:500rpx;
    height:100rpx;
    background:rgba(238,111,105,1);
    border-radius:50rpx;
    text-align: center;
    margin: 84rpx auto;
    color: #fff;
    font-family: PingFang-SC;
    line-height: 100rpx;
}
