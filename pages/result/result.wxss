.result-status {
  position: relative;
}

.result-status >view {
  position: absolute;
  right: 102rpx;
  width: 268rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: rgba(241, 248, 237, 1);
  border-radius: 5rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Regular, PingFang-SC;
  font-weight: 400;
  color: rgba(123, 200, 119, 1);
  text-align: center;
  animation-name: moveRight; /*动画的名称 */
  animation-duration: 2000ms; /*动画从开始到结束的时间*/
  animation-timing-function: linear; /*动画执行快慢的参数*/
  animation-iteration-count: infinite; /*动画执行多少次的参数*//*以下是兼容ios所需，参数意义与上相同*/
  -webkit-animation-name: moveRight;
  -webkit-animation-duration: 2000ms;
  -webkit-animation-timing-function: linear;
  -webkit-animation-iteration-count: infinite;
}
@keyframes moveRight {
  from {
    right: 102rpx;
  }

  to {
    right: 55rpx;
  }
}
.triangle {
  width: 0;
