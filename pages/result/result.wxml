<view class="result-top">
    <image class="result-img1" src="../../images/result/circle.png"></image>
    <text  class="result-text1">最近一次</text>
    <image class="result-img2" bindtap="toBind" src="../../images/result/bindInfo.png"></image>
</view>
<view class='content'>
    <canvas canvas-id="bg" class='canvasII'></canvas>
    <canvas canvas-id='dashboard' id='canvas-one'  class='canvasI'></canvas>
    <image class="fatType1" src="{{fatType}}"></image>
    <view class="fatText">{{fatText}}</view>
    <view class="fatTextEn">{{fatTextEn}}</view>
    <view class="fatDes">"{{fatDes}}"</view>
</view>
<button class="result-btn" bindtap="toIndex">吹一吹</button>
<view class="result_tab">
    <view class="{{currenttab==='0'?'tab_item_active':'tab_item'}}" data-tabid="0" bindtap='selectTab'>燃脂记录</view>
    <view class="{{currenttab==='1'?'tab_item_active':'tab_item'}}" style="margin-left: 61rpx" data-tabid="1" bindtap='selectTab'>燃脂趋势</view>
    <view class="clear"></view>
</view>

<!--热门回答  -->
<view wx:if="{{currenttab==='0'}}">
    燃脂记录
</view>

<!--全部分类 -->
<view wx:if="{{currenttab==='1'}}" >
    燃脂趋势
</view>


