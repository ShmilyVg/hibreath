/* pagesIndex/howEat/howEat.wxss */
page{
    box-sizing: border-box;
    padding: 55rpx 30rpx;
}
.howEatV{
    width: 100%;
    height: 166rpx;
    box-sizing: border-box;
    padding: 40rpx 38rpx 45rpx 42rpx;
    background: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size:100% 100%;
}
.howEatT{
    font-size:30rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(224,196,139,1);
    margin-bottom: 10rpx;
}
.howEatB{
    font-size:24rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(224,196,139,1);
    opacity:0.6;
}
.howEatBtn{
    float: right;
    width:138rpx;
    height:54rpx;
    line-height:54rpx;
    background:rgba(224,196,139,1);
    border-radius:27rpx;
    font-size:24rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(69,69,69,1);
    text-align: center;
    margin-top: 15rpx;

}
.howEat-title{
    font-size:36rpx;
    font-family:PingFang SC;
    font-weight:bold;
    color:rgba(51,51,51,1);
    margin-bottom: 20rpx;
    margin-top: 63rpx;
}
.howEat-subtitle{
    font-size:24rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(136,136,136,1);
    margin-bottom: 35rpx;
}
.swiper-view{
    height: 100%;
    width: 500rpx;
    border-radius:10rpx;
    background-repeat:no-repeat;
}
.swiper-bgview{
    width: 100%;
    height: 100rpx;
    background:linear-gradient(0deg,rgba(0,0,0,1),rgba(0,0,0,0));
    opacity:0.3;
    display: flex;
    align-items: center;
    position: relative;
    top:200rpx
}
.swiper-view-span{
    width:59rpx;
    height:28rpx;
    background:linear-gradient(90deg,rgba(246,97,78,1),rgba(244,122,102,1));
    border-radius:4rpx;
    font-size:20rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1);
    margin-left: 20rpx;
    margin-top: 140rpx;
    text-align: center;
}
.swiper-view-title{
    font-size:28rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1);
    margin-left: 30rpx;
    margin-top: 140rpx;
}
