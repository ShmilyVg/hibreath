  right: -45rpx;
  bottom: -44rpx;
}

.item-h4 .vspng {
  width: 30rpx;
}

.item-g-text {
  font-size: 50rpx;
  font-weight: bold;
  color: rgba(53, 192, 80, 1);
  margin-top: 30rpx;
}
.item-g-text .num{
  font-size:80rpx;
  font-family:DINCondensed;
  font-weight:bold;
  color:rgba(53,192,80,1);
}
.item-g-text .unit{
  font-size:36rpx;
  font-weight:500;
  color:rgba(53,192,80,1);
}
.item-n-text {
  font-size: 28rpx;
  
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  margin-top: 40rpx;
}

.weight .item-n-text {
  background: rgba(248, 248, 248, 1);
  padding: 23rpx;
  border-radius: 10rpx;
}

.item-n-text .num{
  font-size:46rpx;
  font-family:DINCondensed;
  font-weight:bold;
  color:rgba(53,192,80,1);
}
.item-n-text .unit{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(53,192,80,1);
}
.item-h4 .fire {
  width: 24rpx;
  height: 30rpx;
}

.burn-progress {
  margin-top: 36rpx;
  display: flex;
  justify-content: space-between;
}

.burn-progress-left {
  padding-top: 25rpx;
  width: 450rpx;
}

.burn-progress-contain {
  width: 450rpx;
  height: 56rpx;
  background: rgba(231, 231, 231, 1);
  box-shadow: 0rpx 6rpx 10rpx 0rpx rgba(0, 0, 0, 0.06);
  border-radius: 28rpx;
}

.burn-value {
  width: 230rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: linear-gradient(90deg, rgba(38, 204, 144, 1), rgba(114, 213, 133, 1));
  border-radius: 28rpx;
  font-size: 24rpx;
  
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  padding-left: 17rpx;
}

.burn-progress-tip {
  margin-top: 24rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
}

.burn-progress-text {
  font-size: 24rpx;
  
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.burn-progress-val {
  font-size: 28rpx;
  
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.burn-progress-right {
  width: 131rpx;
  font-size: 22rpx;
  
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  text-align: center;
}

.burn-progress-right .img-con {
  width: 95rpx;
  height: 95rpx;
  padding-top: 8rpx;
  background: rgba(180, 139, 118, 1);
  border-radius: 50%;
