/* pages/lowFatReport/lowFatReport.wxss */

page {
  background: #f7fcec;
}

view {
  box-sizing: border-box;
  font-family: PingFang SC;
}

.lowFatReportBg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 724rpx;
  z-index: 1;
}

.lowFat {
  padding: 54rpx 30rpx 250rpx;
  position: relative;
  z-index: 2;
}

.lowFat-header {
  font-size: 56rpx;
  
  font-weight: 800;
  color: rgba(255, 255, 255, 1);
  margin-left: 20rpx;
  position: relative;
}

.share {
  height: 50rpx;
  line-height: 50rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 25rpx;
  color: #2ccc8e;
  text-align: center;
  padding: 0 30rpx;
  font-size: 24rpx;
  font-weight: 500;
  position: absolute;
  left: 350rpx;
  top: 20rpx;
}

.copy {
  width: 24rpx;
  height: 24rpx;
}

.lowFat-header-item {
  margin-top: 60rpx;
  position: relative;
  padding-left: 126rpx;
}

.portrait {
  width: 96rpx;
  height: 102rpx;
  border: 4rpx solid #fff;
  border-radius: 50%;
  position: absolute;
  left: 10rpx;
}

.p-name {
  font-size: 32rpx;
  
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  margin-bottom: 15rpx;
}

.p-time {
  font-size: 24rpx;
  
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  opacity: 0.7;
}

.medal {
  position: absolute;
  top: 0;
  right: 15rpx;
  width: 215rpx;
  height: 279rpx;
}

.burnFat {
  margin-top: 60rpx;
}

.lowFat-item {
  background: #fff;
  box-shadow: 0rpx -4rpx 30rpx 0rpx rgba(0, 0, 0, 0.04);
  border-radius: 20rpx;
  padding: 50rpx 30rpx;
}

.item-title {
  font-size: 36rpx;
  
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.item-title-text {
  font-size: 24rpx;
  
  font-weight: 400;
  color: rgba(53, 192, 80, 1);
  display: inline-block;
  margin-left: 30rpx;
}

.item-title-text image {
  width: 20rpx;
  height: 20rpx;
  margin-left: 10rpx;
}

.item-h4 {
  font-size: 28rpx;
  
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-top: 50rpx;
  height: 40rpx;
  line-height: 40rpx;
  position: relative;
  padding-left: 40rpx;
}

.item-h4 >image {
  width: 23rpx;
  height: 30rpx;
  position: absolute;
  left: 10rpx;
  top: 5rpx;
}

.speed-contain {
  position: relative;
  text-align: center;
  margin-bottom: 20rpx;
}

.speed-num {
  position: absolute;
  width: 100%;
  text-align: center;
  font-size: 120rpx;
  font-family: DINCondensed;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  top: 124rpx;
  padding-right: 20rpx;
}

.speed-num view {
  font-size: 30rpx;
  font-family: DINCondensed;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.speedBg {
  width: 526rpx;
  height: 342rpx;
}
.elf-contain{
  width: 350rpx;
  height: 2rpx;
