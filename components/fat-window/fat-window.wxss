/* components/fat-window/fat-window.wxss */
.fat-window-container{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 350%;
    background-color: rgba(0, 0, 0, .9);
    z-index: 995;
    background-size: 100% 1096rpx;
    background-repeat: no-repeat;
}
.fat-window-containerNo{
    background-image: url('https://img.hipee.cn/hibreath/fatWindows/flash1.png')
}
.fat-window-containerActive{
    animation:myOne 0.4s  infinite alternate;
}
@keyframes myOne{
    0% {background-image:url('https://img.hipee.cn/hibreath/fatWindows/flash1.png')}
    100% {background-image:url('https://img.hipee.cn/hibreath/fatWindows/flash2.png')}
}
.clear{
    clear: both;
}
.triangle-right{
    width:0;
    height:0;
    border-top:13rpx solid transparent;
    border-bottom:13rpx solid transparent;
    border-left:20rpx solid rgba(255,255,255,1);
    margin-right: 8rpx;
}
.triangle-left{
    width:0;
    height:0;
    border-top:13rpx solid transparent;
    border-bottom:13rpx solid transparent;
    border-right:20rpx solid rgba(255,255,255,1);
    margin-right: 8rpx;
}
.triangle-right1{
    opacity:0.3;
}
.triangle-right2{
    opacity:0.5;
}
.triangle-right3{
    opacity:0.7;
}
.triangle-active{
    opacity: 1;
}
