    background-repeat: no-repeat;
    background-position:center center;
    margin-top: 30rpx;
    padding-top: 30rpx;
    height: 852rpx;
}
.shareAdd-Span{
    width:111rpx;
    height:50rpx;
    background:rgba(252,236,235,1);
    border-radius:25rpx;
    font-size:28rpx;
    font-family:PingFang-SC-Regular,PingFang-SC;
    font-weight:400;
