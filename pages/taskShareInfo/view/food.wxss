.food .desc{
    padding:40rpx;
    color: #454545;
    font-size:30rpx;
    font-weight: 400;
    text-align: justify;
    box-sizing: border-box;
}

.food .img-list{
    display: flex;
    flex-wrap: wrap;
    box-sizing: border-box;
    padding: 40rpx 40rpx 0rpx 40rpx;
}

.img-list .img-wrap{
    width: 200rpx;
    height: 200rpx;
    overflow: hidden;
    border-radius: 10rpx;
    margin: 5rpx;
}
