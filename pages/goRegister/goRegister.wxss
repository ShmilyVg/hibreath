/* pages/goRegister/goRegister.wxss */
page{
    box-sizing: border-box;
    padding: 110rpx 40rpx 40rpx 40rpx;
    background:linear-gradient(90deg,rgba(244,121,112,1),rgba(245,94,107,1));
}
.goRegister-title{
    font-size: 36rpx;
    color: #fff;
    margin-bottom: 20rpx;
}
.goRegister-subTitle{
    font-size: 28rpx;
    color: #fff;
    margin-bottom: 160rpx;
}
.goRegister-img{
    float: left;
    color: #fff;
    margin-right: 30rpx;
    text-align: center;
    line-height: 100rpx;
    width: 74rpx;
    height: 100rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
}
.goRegister-text{
    margin-top: 10rpx;
    line-height: 45rpx;
    float: left;
    color: #fff;
}
.goRegister-btn{
    border: 1rpx #fff solid;
    color: #fff;
    width: 172rpx;
    height: 53rpx;
    line-height: 53rpx;
    font-size: 28rpx;
    border-radius: 28rpx;
    background: none;
    margin-left: 100rpx;
    margin-top: 55rpx;
}
button:after{
    border: none;
}
