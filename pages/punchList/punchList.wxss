/* pages/punchList/punchList.wxss */
page{
    box-sizing: border-box;
    padding: 28rpx 40rpx 0 40rpx;
}
.punchList_tab{
    width: 100%;
    height: 90rpx;
    background:rgba(245,245,245,1);
    border-radius:20rpx;
    box-sizing: border-box;
    padding: 10rpx;
    text-align: center;
}
/* 头像 */
.punchList_headUrl{
    width:110rpx;
    height:110rpx;
    border-radius: 50%;
    margin:25rpx;
}
.headUrl-box{
    width:150rpx;
    height:150rpx;
    margin-top: 50rpx;
    position:relative;
}
.cell_headUrl-box{
    width:84rpx;
    height:84rpx;
    float: left;
    margin-left: 20rpx;
    margin-right: 14rpx;
    display: flex;
    justify-content: center;
    align-items: center;
   
}
.punchList_tab_top{
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    padding-left: 50rpx;
    padding-top: 20rpx;
    background-color: white;
    width: 700rpx;
    height: 60rpx;
    z-index: 100;
}
.backgroundImg-gold{
  background:url('data:image/png;base64,');
  background-size:84rpx 84rpx;

}
.backgroundImg-silver{
  background:url('data:image/png;base64,');
  background-size:84rpx 84rpx;
}
.backgroundImg-copper{
  background:url('data:image/png;base64,');
  background-size:84rpx 84rpx;
}
/* 上面头像 */
.backgroundImg-goldbig{
  background:url('data:image/png;base64,');
  background-size:150rpx 150rpx;
}
.backgroundImg-silverbig{
  background:url('data:image/png;base64,');
  background-size:150rpx 150rpx;
}
.backgroundImg-copperbig{
  background:url('data:image/png;base64,');
  background-size:150rpx 150rpx;
}


/* 排行图片 */
.punchList_rank_img{
  position:absolute;
  width:50rpx;
  height:40rpx;
  bottom:0;
  left:55rpx;
  text-align:center;
  color:rgba(255,255,255,1);
  font-size:22rpx;
  line-height:40rpx;
}
.greenImg{
  background:url('data:image/png;base64,');
  background-size:50rpx 44rpx;
}
.greenNumImg{
  background:url('data:image/png;base64,');
  background-size:50rpx 44rpx;
}

/* 邀请伙伴围观 */
.punchList_share{
    width:240rpx;
    height:70rpx;
    background:rgba(237,111,105,1);
    border-radius:50rpx;
    font-size:24rpx;
    font-family:PingFang-SC-Regular,PingFang-SC;
    font-weight:400;
    color:rgba(255,255,255,1);
}


.punchList_NickName{
    font-size:28rpx;
    font-family:PingFang-SC-Regular,PingFang-SC;
    font-weight:400;
    color:rgba(69,69,69,1);
    margin-top: 30rpx;
    text-align:center;
    margin-bottom:20rpx;
}
.punchList_rank{
    margin-top: 20rpx;
    width:88rpx;
    height:34rpx;
    background:rgba(10,193,161,1);
    border-radius:17rpx;
    font-size:18rpx;
    font-family:PingFang-SC-Regular,PingFang-SC;
    font-weight:400;
    color:rgba(255,255,255,1);
    line-height:34rpx;
    float: left;
    text-align: center;
}

.punchList-main{
  width:100%;
  height:390rpx;
  display :flex;
  justify-content:space-around;
}
/* 今日减重和累计减重 */
.punchList_Day1{
    width:196rpx;
    float: left;
    margin-top: 100rpx;
    margin-bottom: 80rpx;
}
.punchList_Day2{
   width:196rpx;
    float: left;
    margin-top: 100rpx;
    margin-bottom: 80rpx;
}

.punchList_Day-Text1{
    font-size:60rpx;
    font-family:DINCondensed-Bold,DINCondensed;
    font-weight:bold;
    color:rgba(84,84,84,1);
    margin-left:35rpx;
    line-height:100rpx;
}
.punchList_Day-Text2{
    font-size:22rpx;
    font-family:PingFang-SC-Semibold,PingFang-SC;
    font-weight:600;
    color:rgba(84,84,84,1);
    /* margin-right: 43rpx; */
}
.punchList_Day-Text3{
    width:100%;
    font-size:30rpx;
    font-family:PingFang-SC-Semibold,PingFang-SC;
    font-weight:600;
    color:rgba(84,84,84,1);
    /* margin-right: 43rpx; */
    /* margin-left:55rpx; */
    text-align:center;
    line-height:100rpx;
}
.punchList_Day-Text4{
    width:100%;
    font-size:24rpx;
    font-family:PingFang-SC-Regular,PingFang-SC;
    font-weight:400;
    color:rgba(163,163,163,1);
    display: block;
    text-align:center;
}
.headUrl-content{
  float:left;
  width:240rpx;
  height:410rpx;
  display: flex;
  flex-direction:column;
  align-items:center;
}


.punchList_line{
    width: 1rpx;
    height:50rpx;
    border-right:0.5rpx #D6D6D6 solid;
    float: left;
    margin: 45rpx 20rpx 0rpx 20rpx;
}
.punchList_lineP{
    width: 1rpx;
    height:50rpx;
    border-right:0.5rpx #D6D6D6 solid;
    float: left;
    margin: 45rpx 40rpx 0rpx 40rpx;
}
.tab_item_active{
    width:320rpx;
    height:70rpx;
    background:rgba(237,111,105,1);
    border-radius:15rpx;
    font-size:26rpx;
    font-family:PingFang-SC-Regular,PingFang-SC;
    font-weight:400;
    color:rgba(255,255,255,1);
    line-height: 70rpx;
    float: left;
}
.tab_item{
    float: left;
    width:320rpx;
    height:70rpx;
    line-height: 70rpx;
    font-size:26rpx;
    font-family:PingFang-SC-Regular,PingFang-SC;
    font-weight:400;
    color:rgba(237,111,105,1);
}
.cell{
    width:670rpx;
    height:120rpx;
    border-radius:20rpx;
    z-index: 90;
    margin-bottom: 10rpx;
    padding: 25rpx 30rpx;
    box-sizing: border-box;
    line-height: 70rpx;
}
.cellR{
    background:rgba(252,236,235,1);
}
.cellB{
    background:rgba(245,245,245,1);
}
.cell-rank{
    width:45rpx;
    font-size:40rpx;
    font-family:DINCondensed-Bold,DINCondensed;
    font-weight:bold;
    float: left;
}
.cell-rank text{
  float:right;
}
.cell-rankR{
    color:rgba(237,111,105,1);
}
.cell-rankB{
    color:rgba(84,84,84,1);
}
.cell-title{
    font-size:28rpx;
    font-family:PingFang-SC-Regular,PingFang-SC;
    font-weight:400;
    float: left;
}
.cell-titleR{
    color:rgba(237,111,105,1);
}
.cell-titleB{
    font-weight:400;
}
.cell-date{
    float: right;
    font-family:DINCondensed-Bold,DINCondensed;
    font-weight:bold;
}
.cell-date text:nth-child(1){
    font-size:48rpx;
}
.cell-date text:nth-child(2){
    font-size:22rpx;
}
.cell_headUrl{
    width:70rpx;
    height:70rpx;
    border-radius: 50%;
    margin:3rpx 0 0 3rpx ;
}
.cell-image {
    width:100rpx;
    height:89rpx;
    position: relative;
    z-index: 90;
}
.fatBurningState{
  width:100rpx;
  height:32rpx;
  background:rgba(252,236,235,1);
  border-radius:8rpx;
  font-size:18rpx;
  font-family:PingFang-SC-Regular,PingFang-SC;
  font-weight:400;
  color:rgba(237,111,105,1);
  margin-left:60rpx;
  margin-top:-30rpx;
  text-align:center;
  line-height:32rpx;
}
.fatBurningStateNull{
  width:100rpx;
  height:32rpx;
  margin-left:65rpx;
  margin-top:-30rpx;
  line-height:32rpx;
}
.cellColorGold{
  color:#FFB000;
  
}
.cellColorSilver{
   color:#929FAE;
   
}
.cellColorCopper{
  color:#C58D61;
 
}