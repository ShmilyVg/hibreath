page{
    padding: 0 50rpx;
    box-sizing:border-box;
}
.content{
    width: 100%;
    height: 400rpx;
    margin: 0 auto;
    position: relative;
    text-align: center;
}
.canvasI{
    width: 400rpx;
    height: 400rpx;
    position: absolute;
    left: 0rpx;
    top: 0;
    right: 0;
    margin: auto auto;
    z-index: 2;
}
.canvasII{
    width: 400rpx;
    height: 400rpx;
    position: absolute;
    left: 0rpx;
    top: 0;
    right: 0;
    margin: auto auto;
    z-index: 1;
}

