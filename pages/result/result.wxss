/* pages/result/result.wxss */
page{
    box-sizing:border-box;
    background: #3E3E3E;
    color: #fff;
    cursor: default;
    -webkit-user-select: none;
    user-select: none;
    overflow-x: hidden;
    align-items: center;
}



.background-view {
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    position: absolute;
    padding: 0 40rpx;
}

.bottom-card {
    position: fixed;
    left: 30rpx;
    bottom: 30rpx;
    background-color: white;
    width: 690rpx;
    height: 573rpx;
    border-radius: 50rpx;
}

.bottom-card-title {
    font-weight: 600;
    font-size: 32rpx;
    color: rgba(151, 151, 151, 1);
    line-height: 45rpx;
    margin-top: 48rpx;
    margin-bottom: 45rpx;
}

.resultDate{
    font-size: 24rpx;
    color: #929292;
    text-align: center;
    margin-bottom: 10rpx;
}
.resultText{
    font-size: 26rpx;
    color: #B9B9B9;
    text-align: center;
    margin-top: 10rpx;
}
.small-card {
    width: 140rpx;
    height: 210rpx;
    border-radius: 28rpx;
    align-items: center;
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.background-image {
    position: fixed;
    top: 370rpx;
    right: 0;
    width: 750rpx;
    height: 629rpx;
}

.bottom-card-num {
    width: 46rpx;
    height: 27rpx;
    background: rgba(233, 233, 233, 1);
    border-radius: 18rpx;
    color: #979797;
    font-size: 20rpx;
    line-height: 27rpx;
    text-align: center;
    align-items: center;
    margin-right: 19rpx;
    margin-left: 45rpx;
    margin-top: 8rpx
}
/*按钮发光效果*/



@keyframes myfirst
{
    0%   {box-shadow:0rpx 0rpx 0rpx 0rpx rgba(139, 139, 122, 0.5);}
    25%  {box-shadow:0rpx 0rpx 45rpx 10rpx rgba(205, 201, 201, 0.8);}
    50%  {box-shadow:0rpx 0rpx 90rpx 20rpx rgba(255, 255, 255, 1);}
    75%  {box-shadow:0rpx 0rpx 45rpx 10rpx rgba(205, 201, 201, 0.8);}
    100% {box-shadow:0rpx 0rpx 0rpx 0rpx rgba(139, 139, 122, 0.5);}
}

.resultBtn {
    animation:myfirst 4s linear infinite alternate;
    background-color: #fff;
    width: 136rpx;
    height: 136rpx;
    border-radius: 50%;
}

.resultSJ{
    width: 0;
    height: 0;
    border-left: 12rpx solid transparent;
    border-right: 12rpx solid transparent;
    border-bottom: 26rpx solid #7A7A7A;
    justify-content: center;
    display: flex;
    margin-top: 30rpx;
    margin-bottom: 8rpx;
}
.resultBottomText{
    color:#828282;
    font-size: 28rpx;
    justify-content: center;
    display: flex;
    margin-bottom: 30rpx;
}

.score{
    display:flex;
    flex-direction: row;
    justify-content: center;
    flex-wrap: wrap;
}
.scoreText1 {
    color: white;
    font-weight:bold;
    margin-top: 25rpx;
    font-family:DINCondensed-Bold;
}
.scoreText2 {
    font-size: 20rpx;
    color: white;
    margin-top: 45rpx;
    margin-left: 20rpx;
    font-family:DINCondensed-Bold;
}

.resultHead{
    height: 260rpx;
    width: 100%;
    background-image:url("data:image/png;base64,") ;
    background-repeat:no-repeat;
    background-size:100% 100%;
    -moz-background-size:100% 100%;
}

.resultHeadImg{
    width: 290rpx;
    height: 290rpx;
    margin-top: 50rpx;

}
.resultNumber{
    font-size: 40rpx;
    font-family:DINCondensed-Bold;
}

.resultGrade{
    font-size: 38rpx;
    font-family: DINCondensed-Bold;
}
.result-list{
    float: left;
    width: 33%;
    padding-top: 50rpx;
    height: 86rpx;
}
.resultImg{
    width: 44rpx;
    height: 44rpx;
    margin-left: 50rpx;
    margin-right: 20rpx;
}
.resultLine{
    height: 86rpx;
    width: 23rpx;
}


/* 带图片弹窗模板B */

.modal {
    background-color: rgba(0, 0, 0, 0.4);
    position: fixed;
    flex-direction: column;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 9998;
}

.result-container {
    position: absolute;
    display: flex;
    flex-direction: column;
    width: 60%;
    height: 30%;
    background-color: white;
    border-radius: 10rpx;
}

.title {
    font-size: 34rpx;
    color: black;
    height: 30%;
    text-align: center;
    margin-top: 30rpx;
}

.content {
    margin-top:30rpx;
    padding-left: 50rpx;
    padding-right: 50rpx;
    font-size: 30rpx;
    color: #888888;
    height: 50%;
}

.modal-btn-wrapper {
    position: absolute;
    height: 20%;
    bottom: 0;
    left: 0;
    display: flex;
    flex-direction: row;
    height: 100rpx;
    line-height: 100rpx;
    border-top: 1rpx solid rgba(7,17,27,0.1);
    width: 100%;
}

.cancel-btn, .confirm-btn {
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
    width: 100%;
}

.cancel-btn {
    border-right: 1rpx solid rgba(7,17,27,0.1);
}
/* 带图片弹窗模板E */
