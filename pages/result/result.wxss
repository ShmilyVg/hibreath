    height: 55rpx;
    width: 120rpx;
}
.tab_item{
    font-family: PingFang-SC;
    font-size: 30rpx;
    color: #969696;
    height: 55rpx;
    float: left;
    width: 120rpx;
}
.result-line{
    width: 39rpx;
    border:0.5px #dedede solid ;
