/* pages/low-carbon/low-carbon.wxss */
page{
    background: #7BC877;
    height: 100%;
}
view{
  box-sizing: border-box;
  font-family:PingFang SC;
}
.topView-view1{
    font-size:48rpx;
    font-weight:bold;
    color:rgba(255,255,255,1);
    float: left;
    margin-bottom: 43rpx;
}
.topView-view2{
    width:198rpx;
    height:60rpx;
    line-height: 60rpx;
    text-align: center;
    border:1rpx solid rgba(255,255,255,1);
    border-radius:30rpx;
    font-size:24rpx;
    margin-bottom: 43rpx;
    margin-top: 5rpx;
    font-weight:400;
    float: right;
    color:rgba(255,255,255,1);
}
.contain{
    width:100%;
    min-height: 100%;
    padding: 60rpx 30rpx 80rpx;
    background:rgba(255,255,255,1);
    border-radius:80rpx 0rpx 0rpx 0rpx;
}

.family{
  background: url("data:image/png;base64,");
   width: 690rpx;
    height: 210rpx;
    background-size:100% 100%;
    position: relative;
    margin-bottom: 50rpx;
    padding: 42rpx 44rpx;
}
.family-title{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(129,204,126,1);
  margin-bottom: 16rpx;
}
.family-text{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  margin-bottom: 5rpx;
  opacity: 0.6;
}
.family-btn{
  width:144rpx;
  height:60rpx;
  background:rgba(123,200,119,1);
  border-radius:30px;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:60rpx;
  position: absolute;
  right: 45rpx;
  top: 75rpx;
  text-align: center;
}
.adaptive{
  background:rgba(255,193,113,1);
}
.reduce{
  background:#ADDB71;
}
.consolidate{
  background:#8ED2A4;
  
}
.card{
  padding: 64rpx 20rpx 20rpx;
  border-radius:20rpx;
  position: relative;
  margin-bottom: 30rpx;
}
.card image{
  width: 182rpx; 
  height: 210rpx;
  position: absolute;
  top: 24rpx;
  left: 10rpx;
  z-index: 10;
}
.card-title{
  font-size:40rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  margin-bottom: 57rpx;
  text-align: center;
}
.card-more{
  width:92rpx;
  height:36rpx;
  position: absolute;
  right: 36rpx;
  top: 50rpx;
  z-index: 1000;
}
.card-more image{
  width:92rpx;
  height:36rpx;
}
.card-contain{
  background:rgba(255,255,255,1);
  opacity:0.95;
  border-radius:20rpx;
  padding: 52rpx 44rpx 52rpx 32rpx;
  position: relative;
  z-index: 20;
}
.stepTip{
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  width:120rpx;
  height:38rpx;
  text-align: center;
  line-height: 38rpx;
  background:rgba(85,85,85,1);
  border-radius:18rpx 4rpx 18rpx 4rpx;
  position: absolute;
  left: 31rpx;
  top: -19rpx;
}
.card-text1{
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(68,68,68,1);
}
.card-text2{
  margin-top: 32rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}





















.needMore-title{
  font-size:36rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
  padding-left: 21rpx;
  margin-bottom: 45rpx;
}

.toDo{
    background: url("data:image/png;base64,");
    width: 690rpx;
    height: 384rpx;
    background-size:100% 100%;
}
.toDo-item{
  width: 100%;
  padding: 57rpx 0 0rpx 185rpx;
}
.toDo-view1{
    font-size:32rpx;
    font-weight:bold;
    color:rgba(68,68,68,1);
    margin-bottom: 30rpx;
}
.toDo-view2{
    width:54rpx;
    height:28rpx;
    text-align: center;
    line-height: 28rpx;
    background:rgba(137,212,149,1);
    border-radius:14rpx;
    font-size:24rpx;
    
    margin-right: 16rpx;
    font-weight:500;
    margin-top: 6rpx;
    color:rgba(255,255,255,1);
}
.toDo-view3{
    font-size:26rpx;
    font-weight:400;
    color:rgba(68,68,68,1);
}
.toDo-btn{
    width:578rpx;
    height:82rpx;
    border-radius:41rpx;
    font-size:26rpx;
    background:rgba(123,200,119,1);
    font-weight:500;
    color:#fff;
    text-align: center;
    line-height: 82rpx;
    margin: 43rpx auto 0rpx auto;
    position: relative;
}
.toDo-btn image{
  position: absolute;
  right: 40rpx;
  top: 32rpx;
}








































