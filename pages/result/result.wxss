    top: 178rpx;
    left: 398rpx;
}

.canvas-base-view{
    width: 720rpx;
    margin-left: -25rpx;
    margin-top: 90rpx;
}

.canvas {
    width: 720rpx;
    height: 440rpx;
    margin-top: 50rpx;
}

.canvas-bottom-text{
    font-size: 30rpx;
    color: #718EA0;
}

.cell{
    width: 650rpx;
    height: 180rpx;
    z-index: 90;
}

.cell-title{
    font-size:30rpx;
    font-weight:400;
    color:rgba(69,69,69,1);
    line-height:42rpx;
    margin-top: 44rpx;
}

.cell-date{
