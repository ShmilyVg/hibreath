  position: relative;
}
.foodQuantity{
  width:40rpx;
  height:40rpx;
  background:rgba(104,207,124,1);
  border-radius:50%;
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  position: absolute;
  text-align: center;
  line-height: 40rpx;
  right: -10rpx;
  bottom: 10rpx;
}

.burn-progress-right image {
  width: 80rpx;
  height: 79rpx;
  opacity: 0.64;
}

