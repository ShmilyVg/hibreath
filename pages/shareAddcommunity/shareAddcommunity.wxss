.shareAddcommunity-view{
    text-align: center;
    background-image: url("https://backend.hipee.cn/hipee-resource/images/hibreath/20191105/e395eb36785717d6c18c7b2b4eb6159e.o6zajs-zth1ke_1mwkyso5jiadbc.9yooxhzzhykle395eb36785717d6c18c7b2b4eb6159e.png");
